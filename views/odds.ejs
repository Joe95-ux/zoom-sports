<body>
    <div id="fb-root"></div>
    <script async defer crossorigin="anonymous"
        src="https://connect.facebook.net/en_GB/sdk.js#xfbml=1&version=v14.0&appId=593644877942199&autoLogAppEvents=1"
        nonce="55i7sfzH"></script>

    <nav class="navigation">
        <div class="blog-nav-enclosure">
            <div class="logo-and-links">
                <div class="logo">
                    <div class="burger blog-burger">
                        <div class="line1"></div>
                        <div class="line2"></div>
                        <div class="line3"></div>
                    </div>
                    <a href="/">
                        <div class="logo-wrapper">
                            <img src="/zs-logo.png" alt="Zoom Sportz">
                            <!-- <h1>Zoom Sportz</h1> -->
                        </div>

                    </a>
                </div>

                <ul class="nav_links">
                    <li class="nav_link"> <a href="/">Home</a> </li>
                    <li class="nav_link"> <a href="/odds-comparison" class="<%= live %> ">Odds-comparison</a> </li>
                    <li class="nav_link"> <a href="/live-preview">Livescores</a> </li>
                    <li class="nav_link"> <a href="/highlights">Highlights</a> </li>
                    <li class="nav_link"> <a href="/tables">Tables</a> </li>
                    <li class="nav_link"> <a href="/league">League</a> </li>


                    <% if(user){%>
                    <% if(user.privilege === "admin"){%>
                    <li class="nav_link"> <a href="/users/admin/dashboard/<%= user._id %>">Dashboard</a> </li>
                    <% }else{%>
                    <li class="nav_link"> <a href="/users/dashboard/<%= user._id %>">Dashboard</a> </li>
                    <% } %>
                    <% } %>
                </ul>

            </div>

        </div>
    </nav>

    <div class="side-drawer" data-simplebar>
        <i class="fas fa-times"></i>
        <div class="side-drawer-inner">
            <div class="main-category discover-movies">
                <div class="category-title">
                    <h2 class="discover">Menu</h2>
                </div>
                <div class="category-types">
                    <ul class="categories">
                        <li><a class="active-cat" href="/">Home</a></li>
                        <li><a class="active-cat" href="/odds-comparison">Odds-comparison</a> </li>
                        <li><a class="active-cat" href="/live-preview">Livescores</a> </li>
                        <li><a class="active-cat" href="/highlights">Highlights</a></li>
                        <li><a class="active-cat" href="/tables">Tables</a></li>
                        <li><a class="active-cat" href="/league">League</a></li>
                        <% if(user){%>
                        <% if(user.privilege === "admin"){%>
                        <li><a class="active-cat" href="/users/admin/dashboard/<%= user._id %>">Dashboard</a> </li>
                        <% }else{%>
                        <li> <a class="active-cat" href="/users/dashboard/<%= user._id %>">Dashboard</a> </li>
                        <% } %>
                        <% } %>
                    </ul>
                </div>

            </div>


            <div class="main-category discover-movies">
                <div class="category-title">
                    <h2 class="discover">Categories</h2>
                </div>
                <div class="category-types">
                    <ul class="categories">
                        <% if(sortedCats){%>
                        <% for(let cat in sortedCats){%>
                        <li><i class="fas fa-futbol"></i><a class="active-cat" href="/category/<%=cat%>"><%= cat %></a>
                        </li>
                        <% } %>
                        <% } %>
                    </ul>
                </div>

            </div>

        </div>
        <div class="side-drawer-logo">
            <img src="/zs-logo.png" alt="Zoom Sportz">
        </div>

    </div>
    <section class="side-drawer-backdrop"></section>

    <section id="header-container" class="blog-header">
        <div class="wrapper-enclosure-blog live-preview-wrapper">
            <div class="livescore-wrapper">

                <div id="oddspedia-widget-odds-comparison-popular-false-sports-false-leagues-false">
                    <script>
                        window.oddspediaWidgetOddsComparisonPopularSportsLeagues = {
                            api_token: "<%=token%>",
                            type: "odds-comparison",
                            domain: "zoom-sportz.com",
                            selector: "oddspedia-widget-odds-comparison-popular-false-sports-false-leagues-false",
                            width: "0",
                            theme: "0",
                            odds_type: "1",
                            language: "en",
                            primary_color: "#283E5B",
                            accent_color: "#00B1FF",
                            font: "Roboto",
                            logos: "true",
                            limit: "10",
                            popular: "false",
                            sports: "",
                            leagues: "",
                        };
                    </script>
                    <script
                        src="https://widgets.oddspedia.com/js/widget/init.js?widgetId=oddspediaWidgetOddsComparisonPopularSportsLeagues"
                        async></script>
                </div>

            </div>


            <div class="ads-container">
                <script async
                    src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-7287147359584031"
                    crossorigin="anonymous"></script>
                <!-- banner ads -->
                <ins class="adsbygoogle" style="display:block" data-ad-client="ca-pub-7287147359584031"
                    data-ad-slot="9234749507" data-ad-format="auto" data-full-width-responsive="true"></ins>
                <script>
                    (adsbygoogle = window.adsbygoogle || []).push({});
                </script>
            </div>




        </div>

    </section>







    <%- include('partials/footer') %>